<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length > 0) { %>
                <div class="centered">
                    <ul>
                        <% orders.forEach(order => { %>
                            <li>
                                <h1># <%= order._id %> </h1>
                                <ul>
                                    <% order.products.forEach(item => { %>
                                        <li>
                                            <h3><%= item.product.title %></h3>
                                            <h2><%= item.quantity %></h2>
                                        </li>
                                    <% }) %>
                                </ul>
                            </li>
                        <% }) %>
                    </ul>
                </div>
            <% } else { %>
                <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>